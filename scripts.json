{
    "$schema": "https://deno.land/x/denon@2.5.0/schema.json",
    "scripts": {
        "start": {
            "cmd": "deno run --allow-net --allow-read --allow-write index.ts",
            "desc": "run index.ts file"
        },
        "test": {
            "cmd": "deno test --allow-net index.test.ts",
            "desc": "test index.ts file"
        }
    }
}
